{
  "name": "soa-fe",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run serve:ng",
    "serve": "ng serve --extraWebpackConfig config/webpack.serve.extra.js",
    "serve:ng": "ng serve --project elements-dev -o",
    "build:webpack": "webpack-cli",
    "build:dev": "ng build --extraWebpackConfig config/webpack.extra.js --output-hashing none",
    "build:prod": "ng build --prod --extraWebpackConfig config/webpack.extra.js --output-hashing none",
    "copy:bundle": "cpr dist/soa-elements/main.js deploy/soa/soa.demo.js -o",
    "copy:assets": "cpr dist/soa-elements/assets/less deploy/assets/less -o",
    "build": "npm run build:prod && npm run copy:assets && npm run copy:bundle",
    "deploy": "npm run build && live-server deploy",
    "package": "cat src/assets/base/{native-shim,custom-elements.min,zone.min,rxjs.umd.min,core.umd.min,common.umd.min,platform-browser.umd.min,elements.umd.min}.js | gzip > src/assets/libs/soa.base.js.gz",
    "unpackage": "gunzip -c src/assets/libs/soa.base.js.gz > src/assets/libs/soa.base.js",
    "copy:extra": "cpr water/package.json publish/ -o && cpr water/README.md publish/ -o",
    "publish:water": "tsc -p tsconfig.water.json --outDir publish && npm run copy:extra"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/elements": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@webcomponents/custom-elements": "^1.1.2",
    "core-js": "^2.5.4",
    "document-register-element": "^1.7.2",
    "elements-zone-strategy": "^6.0.4",
    "ng-cosmos-ui": "0.0.5",
    "rxjs": "^6.2.2",
    "water-service": "0.0.2",
    "water-utils": "0.0.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^6.1.1",
    "@angular-devkit/build-angular": "^0.8.4",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "cpr": "^3.0.1",
    "css-loader": "^1.0.0",
    "html-loader": "^0.5.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "live-server": "^1.2.0",
    "ngx-build-plus": "^6.1.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.9.2",
    "webpack-cli": "^3.0.3"
  }
}
